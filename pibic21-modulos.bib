@article{kato:2012paranagua20,
  title = {Paranaguá, {{Antonina}} e {{Curitiba}}, Início Do Século {{XIX}}: Reconstituindo Espaços e a Lógica de Sua Organização Social},
  shorttitle = {Paranaguá, {{Antonina}} e {{Curitiba}}, Início Do Século {{XIX}}},
  author = {Kato, Allan Thomas Tadashi},
  date = {2012-06},
  journaltitle = {Anais do Museu Paulista: História e Cultura Material},
