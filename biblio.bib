}

@inproceedings{matos:2007encontro,
  title = {Um {{Encontro}}, Um {{Desencontro}}. {{Lucio Costa}}, {{Raul Lino}} e {{Carlos Ramos}}},
  shorttitle = {Um {{Encontro}}, Um {{Desencontro}}},
  booktitle = {7º {{Seminário Docomomo Brasil}}},
  author = {Matos, Madalena Cunha and Ramos, Tânia Beisl},
